.weekChoice {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 999;
  opacity: 0.9;
  -webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
}

.box {
  display: grid;
  grid-template-columns: repeat(5, 1fr);
  grid-template-rows: repeat(5, 1fr);
  position: absolute;
  width: 300px;
  height: 300px;
  background-color: #fff;
  border-radius: 1rem;
}

.eachWeek {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50px;
  height: 50px;
  margin: auto;
  font-size: 1.5em;
}

.activeClass {
  border-radius: 1rem;
  color: aliceblue;
  background-color: gray;
}
